package ex01_break;

public class Ex01_break {
	public static void main(String[] args) {
		//break문은 switch를 할 때 나왔던 구문으로
		//case를 종료할 때 사용
		//반복문이 진행되는 도중 특정 조건을 만족해
		//더 이상 반복문을 실행할 필요 없이 종료하려고 할 때 사용
		
		for(int i = 0; i < 10; i++) {
			if(i == 5) {
				break;
			}
			System.out.println(i + " ");
		}
		
		
		
	}
}
