package ex04_array;

public class Exam1 {

	public static void main(String[] args) {
		
		//배열에 각각의 index에 1~10를 할당하고
		//총 합을 구하여 출력
		
		int[] arr = new int[10];
		
		int a = 1;
		int sum = 0;
		
		for(int i = 0; i < arr.length; i++) {
			arr[i] = a;
			sum += arr[i];
			a++;
		}
		System.out.println(sum);
		
		
		
		
		
		
		
		

	}

}
