package ex04_while;

public abstract class Ex02_do_while {
	public static void main(String[] args) {
		// 조건식의 결과와 상관없이 무조건 한 번은 실행한다.
		
		int i = 11;
		do {
			System.out.println(i);
		}while(i <= 10);
		
		// 1~10 총합
		
		i = 1;
		int sum = 0;
		do {
			sum += i;
		}while(i <= 10);
		System.out.println(sum);
		
		
	}
}
