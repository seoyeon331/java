package ex01_print;

public class Ex1_print {
	public static void main(String[] args) {
		System.out.print("welcome");
		System.out.println("welcome"); //줄바꿈 해줌
		
		//문자열 속에서 다른 타입의 데이터를 같이 출력할 수 있게 해주는 출력 형식
		System.out.printf("저는 대학교 %d학년에 재학중입니다.\n", 4); 
		//서식문자를 한번에 여러개 넣을 수도 있다
		
		System.out.printf("%5d",1); //xxxx1
		System.out.println();
		System.out.printf("%.1f\n",1.1234567); //1.1 //반올림됨
  	}
}

