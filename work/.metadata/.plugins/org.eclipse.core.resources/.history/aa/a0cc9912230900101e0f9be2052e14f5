package ex01_multi_array;

public class Ex01_array {
	public static void main(String[] args) {
		int[][] arr = new int[2][3];
		//2차원 배열의 요소는 2개
		
		arr[0][0] = 1;
		arr[0][1] = 2;
		
		
		for(int i = 0; i < arr.length; i++){
			
			for(int j = 0; j < arr[i].length; j++){
						
				System.out.print(arr[i][j] + " ");
						
			}
			System.out.println();		
		}
	}
}
