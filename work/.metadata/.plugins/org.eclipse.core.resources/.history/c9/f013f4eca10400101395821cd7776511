package ex01_first;

public class Test {
	// main 작성 후 ctrl + spacebar
	public static void main(String[] args) {
		// 명령이 끝날때마다 세미콜론 반드시 찍기
		System.out.println("hello");
		
		// sysout 작성 후 ctrl + spacebar
		System.out.println();  
		
		// 아직 해결되지 않은 작업이나 구현해야 할 부분을 나타내는 주석태그
		// TODO : 해야할 일이 있습니다
		
	}
}
