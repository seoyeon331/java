package ex04_exam;

public class MethodTest {
	
		 
		//배열의 최대값을 찾는 maxFinder메서드
		//임의의 배열은 매개변수로 받자
		
		public int max(int[] arr) {
			
			int maxNum = arr[0];
			
			for(int i = 0; i < arr.length; i++) {
				if(arr[i] > maxNum) {
					maxNum = arr[i];
				}
			}
			return maxNum;
		}
		
		
		
		
		
		
		
	
}
