package ex03_for;

public class Ex02_multi_for {
	public static void main(String[] args) {

		// A B C D
		// E F G H
		// I J K L
		
		char x = 'A';
		
		for(int i = 1; i <= 3; i++) {
			for(int j = 1; j <= 4; j++) {
				
				System.out.print(x + " ");
				x++;
				
			}
			System.out.println();
		}
		
		
		
 }
}
